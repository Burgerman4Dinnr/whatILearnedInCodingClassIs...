<%- include('../partials/header'); %>

<main class="main film-main container">
    <div class="row">
        <% data.forEach(item => { %>
            <div class="col-md-2">
                <figure class="figure">
                    <img src="https://via.placeholder.com/150x150" alt="placeholder img" class="img-fluid" />
                    <figcaption>
                        <a href="/film/<%= item.film_id %>">
                            <%= item.title %>
                        </a> 
                        <%= item.rating %>
                    </figcaption>
                </figure>
            </div>
        <% }) %>
    </div>
</main>

<% include('../partials/footer'); %>