<%= include('../partials/header'); %>

<main class="main main-chars">
    <div class="container">
        <div class="row">
            <% data.forEach(item => { %>
                <div class="col-md-4 mb-5 gx-4">
                    <div class="charDiv">
                        <div class="name">
                            <a href="/characters/<%= item.id %>" class="single-char-link"></a>
                             <%= item.title %>
                        </div>
                    </div>
                </div>
           <% }) %>
        </div>
    </div>
</main>




<%= include('../partials/footer'); %>